{
  "manifest_version": 3,
  "name": "webstash",
  "version": "1.0",
  "description": "A simple extension to save your favorite websites",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "dependencies": {
    "exceljs": "^4.4.0"
  }
}
